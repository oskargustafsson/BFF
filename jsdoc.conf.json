{
	"tags": {
		"allowUnknownTags": true
	},
	"plugins": [ "plugins/markdown" ],
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"dateFormat": "MMM YYYY",
		"outputSourceFiles": true,
		"outputSourcePath": true,
		"systemName": "BFF",
		"footer": "",
		"copyright": "DocStrap Copyright Â© 2012-2014 The contributors to the JSDoc3 and DocStrap projects.",
		"navType": "vertical",
		"theme": "lumen",
		"linenums": true,
		"collapseSymbols": false,
		"inverseNav": true,
		"highlightTutorialCode": true,
		"protocol": "html://"
	},
	"markdown": {
		"parser": "gfm",
		"hardwrap": true
	}
}
